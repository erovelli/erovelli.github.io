---
layout: default
---
<div class="post">
  <header class="post-header">
    <h1 class="post-title">
      {{ page.title }}
    </h1>
    {% if page.description %}
      <p class="post-description">{{ page.description }}</p>
    {% endif %}
  </header>

  <article>
    <div class="cv">
      {% assign semesters = site.data.courses.semesters %}
      {% for semester in semesters %}
        <a class="anchor" id="{{ semester.season }}-{{ semester.year }}"></a>
        <div class="card mt-3 p-3">
          <h3 class="card-title font-weight-medium">
            {{ semester.season }}
            {{ semester.year }}
          </h3>
          <div>
            {% include courses/courses.liquid %}
          </div>
        </div>
      {% endfor %}
    </div>
  </article>
</div>
